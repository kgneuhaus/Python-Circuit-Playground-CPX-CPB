def main():
    print("Starting The Code Challenge")
    # Declare Any main() Variables - (Not Global)

    print("Starting <Specific Part> Of Code Challenge")
    # Enter Your Code Here:
    print('Enter Your Code Here:')
    x = 42
    print(x)

    print("Input number y=")
    y = int(input())

    if x == y:
      print("x = y")
    if x != y:
      print("x doesnt = y")
    if x > y:
      print("x > y")
    if x < y:
      print("x < y")
    if x <= y:
      print("x <= y")
    if x >= y:
      print("x >= y")
    if x is y:
      print("x is the same as y")
    if x is not y:
      print("x is not the same as y")

    print("Ending <Specific Part> Of Code Challenge")
    print("Ending The Code Challenge")

main()

main()
