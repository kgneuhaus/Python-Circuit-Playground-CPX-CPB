# sort()
my_list = ["H", "e", "l", "l", "o", " ", "W", "o", "r", "l", "d",]
print(my_list)
# use sort()
my_list.sort()
print(my_list)

# pop()
my_list = ["STEAM", "Clown", "Productions"]
print(my_list)
# use pop()
my_list.pop()
print(my_list)


# del
my_list = ["apple", "orange", "pomegranate", "pineapple"]
print(my_list)
# use del
del my_list[1]
print(my_list)

# remove() "Shadow"
my_list = ["Centauri", "Minbari", "Narn", "Human", "Shadow", "Vorlon"]
print(my_list)
# use remove()
my_list.remove("Centauri")
print(my_list)
