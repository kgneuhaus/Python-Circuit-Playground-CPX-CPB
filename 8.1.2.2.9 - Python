# this is my number guessing program
print('This is a Guessing Game')
print("Ready to play?") 

print("Enter your number:")

number_guess = int(input())
print("Your Guess =", number_guess)
print(type(number_guess))

user_guess = 5
print(type(user_guess))
print("Computer Guess =", user_guess)

if user_guess == number_guess:
  print("Users Guess = Number Guess")

else:
    print("Users Guess isn't equal to the Number Guess")

print('Try Again')
